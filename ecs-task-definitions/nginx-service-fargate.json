{
  "family": "medic-nginx-service",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::832362089079:role/medic-gpu-cluster-task-execution-role",
  "taskRoleArn": "arn:aws:iam::832362089079:role/medic-gpu-cluster-task-role",
  "containerDefinitions": [
    {
      "name": "medic-nginx-container",
      "image": "${ECR_NGINX}:latest",
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/medic-nginx",
          "awslogs-create-group": "true",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "environment": [
        {
          "name": "ALB_HOST",
          "value": "${ALB_HOST}"
        }
      ]
    }
  ]
}